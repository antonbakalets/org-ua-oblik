<?xml version="1.0" encoding="UTF-8"?>
<Context antiJARLocking="true" path="/oblik">
    <Resource name="jdbc/MySQLDS"
              type="javax.sql.DataSource"
              auth="Container"
              driverClassName="com.mysql.jdbc.Driver"
              maxActive="20" maxIdle="10" maxWait="10000"
              username="oblik_user"
              password="oblik_pass"
              url="jdbc:mysql://localhost:3306/oblik_db?useEncoding=true&amp;characterEncoding=UTF-8"/>
</Context>
